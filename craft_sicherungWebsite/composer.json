{
  "require": {
    "craftcms/cms": "3.0.17.1",
    "vlucas/phpdotenv": "^2.4.0",
    "superbig/craft3-mobiledetect": "1.0.1",
    "olivestudio/craft-olivemenus": "1.0.0",
    "craftcms/redactor": "2.1.5",
    "luwes/craft3-codemirror": "v1.0.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
